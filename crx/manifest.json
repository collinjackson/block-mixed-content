{
  "name": "Block Mixed Content",
  "version": "0.2",
  "description": "Block insecure content on secure pages.",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["block-mixed-content.js"],
      "run_at": "document_start"
    }
  ]
}
